                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                         TOP (100) PERCENT J_det_orden_prod.fecha_prod, J_plan_insp_cal.hora, J_plan_insp_cal.cod_detalle, J_orden_prod_tef.consecutivo AS Numero_Orden, J_v_ordenes_prod.nombres, J_Maquinas.Nombre AS Maquina, 
                         J_orden_prod_tef.prod_final, personal.nombres AS Operario, J_orden_prod_tef.cant_prog, J_orden_prod_tef.calidad, J_orden_prod_tef.materia_prima, J_plan_insp_cal.diametro, J_plan_insp_cal.circularidad, 
                         J_plan_insp_cal.peso, J_plan_insp_cal.apariencia, J_plan_insp_cal.resistencia, J_plan_insp_cal.cash, J_plan_insp_cal.helice
FROM            J_orden_prod_tef INNER JOIN
                         J_plan_insp_cal ON J_orden_prod_tef.consecutivo = J_plan_insp_cal.cod_orden INNER JOIN
                         J_v_ordenes_prod ON J_orden_prod_tef.consecutivo = J_v_ordenes_prod.consecutivo INNER JOIN
                         J_Maquinas ON J_orden_prod_tef.id_maquina = J_Maquinas.codigoM INNER JOIN
                         J_det_orden_prod ON J_plan_insp_cal.cod_orden = J_det_orden_prod.cod_orden AND J_plan_insp_cal.cod_detalle = J_det_orden_prod.id_detalle AND J_plan_insp_cal.cod_detalle = J_det_orden_prod.id_detalle INNER JOIN
                         CORSAN.dbo.V_nom_personal_activo AS personal ON J_det_orden_prod.operario = personal.nit
WHERE        (J_orden_prod_tef.consecutivo IS NOT NULL) AND (J_det_orden_prod.fecha_prod &gt;= '2021-04-01') AND (J_det_orden_prod.fecha_prod &lt;= '2021-05-01')
ORDER BY J_plan_insp_cal.cod_detalle</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
